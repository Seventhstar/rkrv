<h2> Create Account</h2>
<%= form_for @user, method: :post do |f| %>
 <div>
  <%= label_tag :username%>
  <%= f.text_field :username%>
 </div>
 <div>
  <%= label_tag :email%>
  <%= f.text_field :email%>
 </div>
 <div>
  <%= label_tag :password%>
  <%= f.text_field :password%>
 </div>
 <div>
  <%= label_tag :password_confirmation%>
  <%= f.text_field :password_confirmation%>
 </div>
 
<%= submit_tag 'Sign Up', class: 'btn btn-default'%>
<%end%>